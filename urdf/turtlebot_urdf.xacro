<?xml version="1.0" ?>
<robot name="turtlebot_urdf" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find turtlebot)/urdf/materials.xacro" />
<xacro:include filename="$(find turtlebot)/urdf/turtlebot_urdf.trans" />
<xacro:include filename="$(find turtlebot)/urdf/turtlebot_urdf.gazebo" />
<xacro:include filename="$(find turtlebot)/urdf/ros2_control.xacro" /> 

<link name="base_link">
  <inertial>
    <origin xyz="-0.011892043852621992 -0.0005619831717906164 0.09178014322322671" rpy="0 0 0"/>
    <mass value="5.029194688421112"/>
    <inertia ixx="0.005252" iyy="0.008834" izz="0.00825" ixy="-8e-06" iyz="2e-06" ixz="-0.000662"/>
  </inertial>
  <visual>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="dark_grey"/>
  </visual>
  <collision>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/base_link.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="wheel_2">
  <inertial>
    <origin xyz="0.06888830561351354 -0.023235974830111583 0.023835792094402966" rpy="0 0 0"/>
    <mass value="0.05"/>
   <inertia ixx="${(2/5) * 0.05 * (0.033*0.033)}" ixy="0.0" ixz="0.0"
                    iyy="${(2/5) * 0.05 * (0.033*0.033)}" iyz="0.0"
                    izz="${(2/5) * 0.05 * (0.033s*0.033)}" />
  </inertial>
  <visual>
    <origin xyz="0.06353 0.0615 -0.034204" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/wheel_2.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="blue"/>
  </visual>
  <collision>
    <origin xyz="0.06353 0.0615 -0.034204" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/wheel_2.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="wheel_1">
  <inertial>
    <origin xyz="3.056135131906945e-07 0.01829722739215528 -1.1901500950733945e-07" rpy="0 0 0"/>
    <mass value="0.05"/>
     <inertia ixx="${(2/5) * 0.05 * (0.033*0.033)}" ixy="0.0" ixz="0.0"
                    iyy="${(2/5) * 0.05 * (0.033*0.033)}" iyz="0.0"
                    izz="${(2/5) * 0.05 * (0.033s*0.033)}" />
  </inertial>
  <visual>
    <origin xyz="0.06353 -0.065361 -0.034204" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="blue"/>
  </visual>
  <collision>
    <origin xyz="0.06353 -0.065361 -0.034204" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/wheel_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="castor_1">
  <inertial>
    <origin xyz="0.01981118107895722 -4.379036331437309e-07 -0.0356085862227856" rpy="0 0 0"/>
    <mass value="0.04497681815229099"/>
    <inertia ixx="1.9e-05" iyy="2.7e-05" izz="1.4e-05" ixy="0.0" iyz="-0.0" ixz="7e-06"/>
  </inertial>
  <visual>
    <origin xyz="-0.05142 0.000539 -0.0695" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/castor_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="-0.05142 0.000539 -0.0695" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/castor_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<link name="rplidar_1">
  <inertial>
    <origin xyz="-0.0018050632757317456 -0.0013635449377829775 0.006379983654134419" rpy="0 0 0"/>
    <mass value="0.02197925742535277"/>
    <inertia ixx="1e-05" iyy="1e-05" izz="1.6e-05" ixy="-0.0" iyz="0.0" ixz="0.0"/>
  </inertial>
  <visual>
    <origin xyz="0.039624 0.000539 -0.161486" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/rplidar_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
    <material name="silver"/>
  </visual>
  <collision>
    <origin xyz="0.039624 0.000539 -0.161486" rpy="0 0 0"/>
    <geometry>
      <mesh filename="file://$(find turtlebot)/meshes/rplidar_1.stl" scale="0.001 0.001 0.001"/>
    </geometry>
  </collision>
</link>

<joint name="left_wheel_joint" type="continuous">
  <origin xyz="-0.06353 -0.0615 0.034204" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="wheel_2"/>
  <axis xyz="-0.0 -1.0 -0.0"/>
</joint>

<joint name="right_wheel_joint" type="continuous">
  <origin xyz="-0.06353 0.065361 0.034204" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="wheel_1"/>
  <axis xyz="-0.0 -1.0 0.0"/>
</joint>

<joint name="castor_joint" type="fixed">
  <origin xyz="0.05142 -0.000539 0.0695" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="castor_1"/>
</joint>

<joint name="rplidar_joint" type="fixed">
  <origin xyz="-0.039624 -0.000539 0.161486" rpy="0 0 0"/>
  <parent link="base_link"/>
  <child link="rplidar_1"/>
</joint>

</robot>
